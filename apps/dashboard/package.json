{
	"name": "@answeroverflow/dashboard",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"with-env": "dotenv -e ../../.env --",
		"dev": "pnpm with-env next dev --port 3002 --turbo",
		"build": "pnpm with-env next build",
		"clean": "rm -rf .next .turbo node_modules",
		"start": "pnpm with-env next start --port 3002",
		"format": "prettier --ignore-path ../../.gitignore --config .prettierrc.mjs --write .",
		"format:check": "prettier --ignore-path ../../.gitignore --config .prettierrc.mjs --check .",
		"lint": "pnpm with-env next lint && tsc --noEmit",
		"lint:fix": "pnpm with-env next lint --fix",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@answeroverflow/api": "workspace:^",
		"@answeroverflow/auth": "workspace:^",
		"@answeroverflow/constants": "workspace:^",
		"@answeroverflow/db": "workspace:^",
		"@answeroverflow/env": "workspace:^",
		"@answeroverflow/hooks": "workspace:^",
		"@answeroverflow/tailwind-config": "workspace:^",
		"@answeroverflow/ui": "workspace:^",
		"@aws-sdk/client-s3": "^3.577.0",
		"@heroicons/react": "^2.0.18",
		"@tanstack/react-query": "^4.36.1",
		"@tanstack/react-table": "^8.17.3",
		"@tremor/react": "^3.6.1",
		"@trpc/server": "^10.45.2",
		"@typelytics/tremor": "^0.1.6",
		"next": "15.0.0-canary.128",
		"next-auth": "^4.23.1",
		"posthog-js": "^1.83.0",
		"react": "19.0.0-rc-6230622a1a-20240610",
		"react-dom": "18.3.1",
		"react-icons": "^4.10.1",
		"react-toastify": "^10.0.5",
		"remark-gfm": "^3.0.1",
		"sharp": "^0.32.5",
		"stripe": "^12.18.0",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@answeroverflow/eslint-config-custom": "workspace:^",
		"@answeroverflow/prettier-config": "workspace:^",
		"@answeroverflow/tsconfig": "workspace:^",
		"@edge-runtime/vm": "^2.1.2",
		"@headlessui/tailwindcss": "^0.2.0",
		"@types/node": "^20.11.20",
		"@vitest/coverage-v8": "^2.0.5",
		"@types/react": "npm:types-react@rc",
		"@types/react-dom": "npm:types-react-dom@rc",
		"@vitest/coverage-c8": "^0.30.1",
		"autoprefixer": "^10.4.15",
		"dotenv-cli": "^7.4.1",
		"eslint": "^8.57.0",
		"postcss": "^8.4.28",
		"prettier": "^3.3.3",
		"sass": "^1.66.1",
		"tailwindcss": "^3.3.3",
		"vitest": "^2.0.5"
	},
	"overrides": {
		"@types/react": "npm:types-react@rc",
		"@types/react-dom": "npm:types-react-dom@rc"
	}
}
